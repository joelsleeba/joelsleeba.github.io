@article{Seiferas2001,
  abstract = {For any fixed \textdollar k\textdollar , a remarkably simple single-tape Turing machine can simulate \textdollar k\textdollar  independent counters in real time. Informally, a counter is a storage unit that maintains a single integer (initially 0), incrementing it, decrementing it, or reporting its sign (positive, negative, or zero) on command. Any automaton that responds to each successive command as a counter would is said to simulate a counter. (Only for a sign inquiry is the response of interest, of course. And zeroness is the only real issue, since a simulator can readily use zero detection to keep track of positivity and negativity in finite-state control. In this paper we describe a remarkably simple real-time simulation, based on just five simple rewriting rules, of any fixed number \textdollar k\textdollar  of independent counters. On a Turing machine with a single, binary work tape, the simulation runs in real time, handling an arbitrary counter command at each step. The space used by the simulation can be held to \textdollar (k+\textbackslash epsilon) \textbackslash log\_2 n\textdollar  bits for the first \textdollar n\textdollar  commands, for any specified \textdollar \textbackslash epsilon > 0\textdollar .},
  author = {Seiferas, Joel and Vitanyi, Paul},
  eprint = {cs/0110038v1},
  eprintclass = {cs.CC},
  eprinttype = {arxiv},
  journal = {J. Seiferas and P.M.B. Vitanyi, Counting is easy, J. Assoc. Comp.
  Mach. 35 (1988), pp. 985-1000},
  month = {10},
  title = {Counting is Easy},
  url = {http://arxiv.org/abs/cs/0110038v1},
  year = {2001},
}

