@article{Clouatre2023,
  abstract = {Through the lens of non-commutative function theory, we examine a non-selfadjoint variant of the Kadison--Singer problem on extensions of pure states. More precisely, given a unital subspace \textdollar M\textdollar  of a \textdollar C\^{}\ast \textdollar -algebra \textdollar B\textdollar , we aim to characterize those pure states on \textdollar M\textdollar  admitting a unique extension to a state on \textdollar B\textdollar . We find naturally occurring examples where every pure state has this property. In the more structured setting of a subalgebra \textdollar A\textdollar  of \textdollar B\textdollar , we shed light on the uniqueness of extensions via the existence of excisions in \textdollar A\textdollar , in the sense of Akemann--Anderson--Pedersen. Certain non-commutative peak points, both in the state space and in the representation space of \textdollar B\textdollar , play a crucial role in our analysis. In particular, the question of abundance of non-commutative peak points is addressed.},
  author = {Clou{\^a}tre, Rapha{\"e}l},
  eprint = {2306.02365v2},
  eprintclass = {math.OA},
  eprinttype = {arxiv},
  month = {6},
  title = {Pure states on operator algebras: excision, extension and peaking},
  url = {http://arxiv.org/abs/2306.02365v2},
  year = {2023},
}

