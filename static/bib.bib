@article{Chiribella2022,
  abstract = {Arveson\textquotesingle s extension theorem guarantees that every completely positive map defined on an operator system can be extended to a completely positive map defined on the whole C\ast -algebra containing it. An analogous statement where complete positivity is replaced by positivity is known to be false. A natural question is whether extendibility could still hold for positive maps satisfying stronger conditions, such as being unital and norm 1. Here we provide three counterexamples showing that positive norm-one unital maps defined on an operator subsystem of a matrix algebra cannot be extended to a positive map on the full matrix algebra. The first counterexample is an unextendible positive unital map with unit norm, the second counterexample is an unextendible positive unital isometry on a real operator space, and the third counterexample is an unextendible positive unital isometry on a complex operator space.},
  author = {Chiribella, Giulio and Davidson, Kenneth R. and Paulsen, Vern I. and Rahaman, Mizanur},
  doi = {10.1016/j.laa.2023.01.003},
  eprint = {2204.08819v1},
  eprintclass = {math.OA},
  eprinttype = {arxiv},
  journal = {Linear Algebra and its Applications (2023)},
  month = {4},
  title = {Counterexamples to the extendibility of positive unital norm-one maps},
  url = {http://arxiv.org/abs/2204.08819v1},
  year = {2022},
}

@article{Hataya2023,
  abstract = {We propose a new generalization of neural networks with noncommutative \textdollar C\^{}\ast \textdollar -algebra. An important feature of \textdollar C\^{}\ast \textdollar -algebras is their noncommutative structure of products, but the existing \textdollar C\^{}\ast \textdollar -algebra net frameworks have only considered commutative \textdollar C\^{}\ast \textdollar -algebras. We show that this noncommutative structure of \textdollar C\^{}\ast \textdollar -algebras induces powerful effects in learning neural networks. Our framework has a wide range of applications, such as learning multiple related neural networks simultaneously with interactions and learning invariant features with respect to group actions. We also show the validity of our framework numerically, which illustrates its potential power.},
